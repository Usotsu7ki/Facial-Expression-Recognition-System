
#frame_background{
    background:url(:/res/pic/login1.jpg);
}
/*登录背景*/
#frame_login{background-color:#ffffff}

/*右侧图片*/
#frame_pic{
    background-image:url(:/res/pic/right.jpg);
    background-position: center center;
    background-repeat: no-repeat;  /* 禁止背景图像重复 */
}
/*账户和密码框*/
#frame_pwd{
    border: 1px solid #696969;
    border-radius: 5px;
}

QLineEdit{
    border: none;
}


#btn_forget{
    background-color:rgba(255, 255, 255, 0);
    border:none;
    color:#080808;
}

/*按钮-user?-鼠标悬停*/
#btn_forget:hover{
    border:none;
    color:#e83737;
}

